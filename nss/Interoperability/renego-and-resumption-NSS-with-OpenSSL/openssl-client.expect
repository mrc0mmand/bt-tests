#!/usr/bin/expect -d
set timeout 10
spawn /bin/sh -c "$argv"
expect {
    "Verify return code: 0 " {
        send "GET / HTTP/1.0\r\r"
        expect "Server: Generic Web Server" {
            puts "Before close"
            close
            puts "Before exit"
            exit 0
        }
    }
}
exit 1
