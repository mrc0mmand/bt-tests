#!/usr/bin/expect
set timeout 15
spawn /bin/sh -c "$argv"
expect {
    "Enter Password" { send "RedHatEnterpriseLinux6.6\r"; exp_continue }
    eof { }
    "subject DN" { expect "subject DN" { send "GET / HTTP/1.0\r\r";
                                         expect -timeout 5 "Verify return code";
                                         close}}
}
set info [wait]
#puts "Return from wait: $info"
exit [lindex $info 3]
